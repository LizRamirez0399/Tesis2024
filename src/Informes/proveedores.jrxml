	<queryString>
		<![CDATA[SELECT 
P.idproveedor,
P.razon_social,
P.ruc,
P.representante_legal,
P.telefono,
P.direccion,
CASE
	WHEN P.estado = 'A' THEN 'ACTIVO'
	WHEN P.estado = 'I' THEN 'INACTIVO'
	ELSE 'INDEFINIDO'
END ESTADO,
P.idciudad,
CI.descripcion AS CIUDAD_DESC
FROM proveedor AS P
INNER JOIN ciudad AS CI ON CI.idciudad = P.idciudad;]]>
	</queryString>
